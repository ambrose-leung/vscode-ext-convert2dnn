{
  "name": "convert2dnn",
  "displayName": "LinqPad Sript to .NET Notebook",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:convert2dnn.convert2DotNetNotebook"
  ],
  "main": "./extension.js",
  "contributes": {
    "":{"comment":"contribution points: https://code.visualstudio.com/api/references/contribution-points"},
    "menus": {
      "commandPalette": [
        {
          "comment":"when clauses: https://code.visualstudio.com/api/references/when-clause-contexts",
          "command": "convert2dnn.convert2DotNetNotebook",
          "when": "resourceExtname == .linq"
        }
      ],
      "editor/context": [
        {
          "command": "convert2dnn.convert2DotNetNotebook",
          "when": "resourceExtname == .linq"
        }
      ]
    },
    "commands": [
      {
        "command": "convert2dnn.convert2DotNetNotebook",
        "title": "Convert to .NET Interactive Notebook (.ipynb)"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "fast-xml-parser": "^3.21.1"
  }
}
